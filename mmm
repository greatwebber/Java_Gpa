import java.util.*;
public class MyClass {
    public static void main(String args[]) {
     Scanner Input = new Scanner(System.in);
     String FirstName, SecondName, MatricNum;
     int userInput;
       //Enter Your Name
     System.out.println("Enter Your First Name: ");
     FirstName = Input.nextLine();
      //Enter Your Second Name
     System.out.println("Enter Your Second Name: ");
     SecondName = Input.nextLine();
      //Enter Matric No
     System.out.println("Enter Your Matric No: ");
     MatricNum = Input.nextLine();
     
     
     String [] arrEx = {"COM121", "Sara", "Matt", "Anna", "Sofie"};
        // Declare scanner objects to save input
        Scanner in = new Scanner(System.in);
        System.out.println("Enter Score For COM121");
        // Saves the integer the user enters
        userInput = in.nextInt();
        // Calls the method
        System.out.println(arrEx[userInput]);
    
    }
}